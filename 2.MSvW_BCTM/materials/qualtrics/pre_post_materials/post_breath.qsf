        jslib_url + \"plugins\/jspsych-canvas-keyboard-response.js\",\n        jslib_url + \"plugins\/jspsych-fullscreen.js\"\n    ];\n    \n    function loadScript(idx) {\n        console.log(\"Loading \", requiredResources[idx]);\n        jQuery.getScript(requiredResources[idx], function () {\n            if ((idx + 1) < requiredResources.length) {\n                loadScript(idx + 1);\n            } else {\n                initExp();\n            }\n        });\n    }\n\n    if (window.Qualtrics && (!window.frameElement || window.frameElement.id !== \"mobile-preview-view\")) {\n        loadScript(0);\n    }\n\n    \/* Change 3: Appending the display_stage Div using jQuery *\/\n    \/\/ jQuery is loaded in Qualtrics by default\n    jQuery(\"<div id = 'display_stage_background'><\/div>\").appendTo('body');\n    jQuery(\"<div id = 'display_stage'><\/div>\").appendTo('body');\n\n\nvar sbj_id = \"${e:\/\/Field\/Random_ID}\";\nvar save_url = \"https:\/\/users.sussex.ac.uk\/mel29\/exp_data\/save_data_metacog.php\";\n\nfunction save_data_json(my_dataset,data_vers){\n    jQuery.ajax({\n        method: 'POST',\n        dataType: 'json',\n        cache: false,\n        url: save_url,\n        data: {\n            file_name: sbj_id + data_vers + '.json', \/\/ the file type should be added\n            exp_data: my_dataset.json()\n        }\n    });\n}\n    \/* Change 4: Wrapping jsPsych.init() in a function *\/\nfunction initExp() {\n\n\/\/EXPERIMENTAL VARIABLES\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\n    var timeline = [];\n    var resp_keys = ['e','i'];\n\n    var squareWidth = 250;\n    var dot_size = 2;\n    var nDots = 4.25;\n\n    var trial_num = 1;\n    var block_num = 1;\n\n    var num_prac = 26; \/\/if testing, note trials 6 and 11 are used by staircase changes\n    var num_trial = 168; \/\/if testing, must be multiple of 5 as 5 blocks are used\n    var num_block = 4;\n    var num_total = num_prac+num_trial;\n\n    \/\/box position\n    var left_x = 75;\n    var right_x = 475;\n    var both_y = 145;\n\n\/\/TRAINING INFO\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\n    var welcome = {\n      type: 'html-keyboard-response',\n      stimulus: `Welcome to the task!<br>\n        We will now ask you to judge which of two images contains more dots, before asking you to rate your confidence in your judgement.<br>\n        At the beginning of each trial, you will be presented with a black cross in the middle of the screen. Focus your attention on it. Then, two black boxes with a number of white dots will be flashed and you will be asked to judge which box had a higher number of dots.<br><br>\n        If the box on the <strong>left<\/strong> had more dots, <strong>press  ` + resp_keys[0] + ` <\/strong>.<br> If the box on the <strong>right<\/strong> had more dots, <strong>press  ` + resp_keys[1] + ` <\/strong>.<br><br>\n        Please respond quickly and to the best of your ability.<br>\n        You will then rate your confidence in your judgement on a scale with the mouse.<br>\n        Please do your best to rate your confidence accurately and do take advantage of the whole rating scale.<br><br>\n        Press spacebar to continue.`,\n      choices: [' ']\n    };\n\n    var prac_intro = {\n      type: 'html-keyboard-response',\n      stimulus: `We will now ask you to carry out some practice trials. Please respond only when the dots have disappeared.<br>\n        In this practice phase we will tell you whether your judgements are right or wrong.<br><br>\n        If you are <strong>correct<\/strong>, the box that you selected will be outlined in <font color=\"green\"><strong>green<\/strong><\/font>.<br>\n        If you are <strong>incorrect<\/strong>, the box that you selected will be outlined in <font color=\"red\"><strong>red<\/strong><\/font>.<br><br>\n        You will not need to rate your confidence of your judgements on these trials.<br><br>\n        Press spacebar to continue.`,\n      choices: [' ']\n    };\n\n    var no_feedback = {\n      type: 'html-keyboard-response',\n      stimulus: `In the task proper, you will not be provided accuracy feedback on your judgements, but the box you selected will be outlined in <font color=\"blue\"><strong>blue<\/strong><\/font>.<br>\n      You will be asked to rate your confidence in your judgement on a rating scale after each trial, which will be explained next.<br><br>Press spacebar to continue.`,\n      choices: [' ']\n    };\n\n    var conf_intro = {\n        type: 'html-slider-response',\n        stimulus:`A rating scale as shown below is used throughout the task.<br>You will be able to rate your confidence of your judgements by choosing any point along the rating scale with your mouse.<br><br>`,\n        prompt: `Choose any point on the rating scale and click \u2018Submit\u2019 to continue.<br><br>`,\n        min: 1, max: 6, slider_start: 3, slider_width: 800,\n        labels: ['1<br>Guessing','2','3','4','5','6<br>Certainly correct'],\n        button_label: 'Submit',\n    };\n\n    var exp_cont = {\n      type: 'html-keyboard-response',\n      stimulus: `You will now continue directly to the experiment. The dots will presented only for a short period of time.<br>\n    You will be asked to rate your confidence in your judgement after each trial.<br><br>\n    Press spacebar to continue.`,\n      choices: [' ']\n    };\n\n    var exp_intro = {\n      type: 'html-keyboard-response',\n      stimulus: `The task proper is divided into 4 blocks of 42 trials each, where you \n                    can pause for a break before every block. There are no time limits on your responses to the dots and on your confidence ratings. <br><br>\n                    As a reminder: If the box on the left had more dots, <strong>press  ` + resp_keys[0] + ` <\/strong>. If the box on the right had more dots, <strong>press  ` + resp_keys[1] + ` <\/strong>.<br><br>\n                    Press spacebar to begin!`,\n      choices: [' ']\n    };\n    \n    var end_prac = {\n        timeline: [no_feedback,conf_intro,exp_cont,exp_intro],\n        conditional_function: function(){\n            if(trial_num == num_prac+1) {\n                return true\n            } else {\n                return false}\n        }\n    };\n\n\/\/TRIALS\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\n    var fixation = {\n            type: 'html-keyboard-response',\n            stimulus: '<div style=\"font-size:60px;\">+<\/div>',\n            choices: jsPsych.NO_KEYS,\n            trial_duration: 1000,\n        };\n\n    \/\/STIM\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\n    function drawDots(ctx, x_coord, nDots_draw){\n        var cellSize = 10;\n        var dotIndex = jsPsych.randomization.shuffle([...Array(625).keys()]);\n        var mat_no = 0;\n        for (var x = x_coord ; x < x_coord + squareWidth; x += cellSize){\n            for (var y = both_y ; y < both_y + squareWidth; y += cellSize){\n                if (dotIndex[mat_no] < (312 + nDots_draw)){\n                   ctx.beginPath() \/\/ stops tearing of the graphics\n                   ctx.fillStyle = \"white\";\n                   ctx.arc(x + (cellSize\/2) , y + (cellSize\/2), dot_size, 0, Math.PI*2);\n                   ctx.fill();\n                }\n                mat_no++;\n            }\n        }\n    };\n\n    function drawStim(c, side, nDots_stim){\n        var ctx = c.getContext('2d');\n        if (side == 'left'){\n            var target = left_x;\n            var non_target = right_x;\n        } else { \n            var target = right_x;\n            var non_target = left_x;\n        };\n        ctx.fillRect(target, both_y, squareWidth, squareWidth);\n        ctx.fillRect(non_target, both_y, squareWidth, squareWidth);\n        drawDots(ctx, target, nDots_stim);\n        drawDots(ctx, non_target, 0);\n    };\n\n    function staircase(current_dots, back_1, back_2, trial_num_stair){\n        if (trial_num_stair < 7){ \n            var step_size = .4;\n        } else if (trial_num_stair > 6 && trial_num_stair < 12){\n            var step_size = .2\n        } else if (trial_num_stair > 11){\n            var step_size = .1};\n\n        if (back_1 == true && back_2 == true){\n            current_dots -= step_size\n        } else if (back_1 == false){\n            current_dots += step_size};\n        \/\/if (current_dots < 1){current_dots = 1} \n        \/\/else if (current_dots > 4.25){current_dots = 4.25};\n        return(current_dots)\n    }\n\n    var dots = {\n        type: 'canvas-keyboard-response',\n        canvas_size: [540, 800],\n        stimulus: function(c){\n            if (trial_num > 2){\n                var two_trial_ago = jsPsych.data.get().filter({task: 'choice'}).last(2).values()[0].correct\n            };\n            if (trial_num > 1){ \/\/ if statement stops error being thrown with .correct property\n                var one_trial_ago = jsPsych.data.get().filter({task: 'choice'}).last(1).values()[0].correct\n            };\n            var target_side = jsPsych.timelineVariable('stimulus');\n            nDots = staircase(nDots, one_trial_ago, two_trial_ago, trial_num);\n            drawStim(c, target_side, Math.round(Math.exp(nDots)));\n        },\n        prompt: `<p style=\"position:relative; top:150px\"><strong>Press  ` + resp_keys[0] + ` <\/strong> if the box on the left had more dots.<br><strong>Press  ` + resp_keys[1] + ` <\/strong> if the box on the right had more dots.<\/p>`,\n        choices: jsPsych.NO_KEYS,\n        trial_duration: 150,\n        data: {\n        task: 'dots',\n        },\n        on_finish: function(data) {data.number_dots = nDots} \/\/run in on_finish as functions are called before trial otherwise\n    };\n\n        var dots_rep = {\n        type: 'canvas-keyboard-response',\n        canvas_size: [540, 800],\n        stimulus: function(c){\n            var target_side = jsPsych.timelineVariable('stimulus');\n            drawStim(c, target_side, Math.round(Math.exp(nDots)));\n        },\n        prompt: `<p style=\"position:relative; top:150px\"><strong>Press  ` + resp_keys[0] + ` <\/strong> if the box on the left had more dots.<br><strong>Press  ` + resp_keys[1] + ` <\/strong> if the box on the right had more dots.<\/p>`,\n        choices: jsPsych.NO_KEYS,\n        trial_duration: 150,\n    };\n\n\n    \/\/POST-STIM\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\n    var choice = {\n        type: 'canvas-keyboard-response',\n        canvas_size: [540, 800],\n        stimulus: function (c){\n                    var ctx = c.getContext('2d');\n                    ctx.fillRect(left_x, both_y, squareWidth, squareWidth);\n                    ctx.fillRect(right_x, both_y, squareWidth, squareWidth);\n                },\n        choices: resp_keys,\n        prompt: `<p style=\"position:relative; top:150px\"><strong>Press  ` + resp_keys[0] + ` <\/strong> if the box on the left had more dots.<br><strong>Press  ` + resp_keys[1] + ` <\/strong> if the box on the right had more dots.<\/p>`,\n        data: {\n            task: 'choice',\n            trial_number: trial_num,\n            target_side: jsPsych.timelineVariable('stimulus'),\n            correct_response: jsPsych.timelineVariable('correct_response')\n        },\n        on_finish: function(data){\n            \/\/ Score the response as correct or incorrect.\n            if(jsPsych.pluginAPI.compareKeys(data.response, jsPsych.timelineVariable('correct_response'))){\n              data.correct = true;\n            } else {\n              data.correct = false;\n            };\n            if (trial_num <= num_prac){ \/\/if statements can't be used in data:{}\n                data.trial_prac = 'practice';\n            } else {\n                data.trial_prac = 'experimental';\n            }\n        }\n    }\n\n    var feedback = {\n        type: 'canvas-keyboard-response',\n        canvas_size: [540, 800],\n        stimulus: function (c){\n                var ctx = c.getContext('2d');\n                ctx.fillRect(left_x, both_y, squareWidth, squareWidth);\n                ctx.fillRect(right_x, both_y,squareWidth, squareWidth);\n                if (trial_num <= num_prac){\n                    if(jsPsych.data.get().last(1).values()[0].correct){\n                        ctx.fillStyle = '#00ff00'\n                    } else {ctx.fillStyle = '#ff0000'};\n                } else {ctx.fillStyle = '#0000ff'}     \n\n                if (jsPsych.data.get().last(1).values()[0].response == resp_keys[0]){\n                    ctx.fillRect(left_x, both_y, squareWidth, squareWidth)\n                } else {\n                    ctx.fillRect(right_x, both_y, squareWidth, squareWidth)\n                }\n        },\n        choices: jsPsych.NO_KEYS,\n        trial_duration: 500,\n        on_finish: function(data){trial_num++}\n    }\n\n    var confid = {\n        type: 'html-slider-response',\n        stimulus:'Rate your confidence',\n        min: 1, max: 6, slider_start: 3, slider_width: 800,\n        labels: ['1<br>Guessing','2','3','4','5','6<br>Certainly correct'],\n        button_label: 'Submit',\n        data: {\n            task: 'confid'} \/\/tag for easier analysis\n    };\n\n    var conf = {\n        timeline: [confid],\n        conditional_function: function(){\n            if(trial_num > num_prac+1){\n                return true\n            } else {\n                return false}\n        }\n    };\n\n    \/\/BREAK\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\n    var trial_break = {\n        type: 'html-keyboard-response',\n        stimulus: function(){ \/\/must be a function to return the updated block_num\n             return `You can now pause for a break. You have completed ` + block_num + ` out of ` + num_block + \n             ` blocks.<br><br> As a reminder:<br>If the box on the left had more dots, <strong>press  ` + resp_keys[0] + \n             ` <\/strong>.<br> If the box on the right had more dots, <strong>press  ` + resp_keys[1] + ` <\/strong>.<br><br>Press spacebar to continue the task`},\n        choices: [' '],\n        on_finish: function(data){block_num++}\n    };\n\n    console.log(nDots)\n    var block = {\n        timeline: [trial_break],\n        conditional_function: function(){\n            if(((trial_num-1)-num_prac)%(num_trial\/num_block) == 0 && trial_num > (num_prac+1) && trial_num < num_total) {\n                return true\n            } else {\n                return false}\n        }\n    };\n\n    \/\/CREATING EXPERIMENT\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\n    var trial_procedure = {\n        timeline: [end_prac, block, fixation, dots,dots_rep,dots_rep,dots_rep,dots_rep, choice, feedback, conf],\n        timeline_variables: [{stimulus: 'left', correct_response: resp_keys[0]},\n                             {stimulus: 'right', correct_response: resp_keys[1]}],\n        repetitions: num_total\/2,\n        randomize_order: true,\n        type: 'with-replacement'\n    };\n\n    timeline.push({ \/\/prompts full screen\n        type: 'fullscreen',\n        fullscreen_mode: true\n    });\n\n    timeline.push(welcome, prac_intro, trial_procedure);\n\n    jsPsych.init({\n      timeline: timeline,\n      display_element: 'display_stage',\n        on_finish: function() {\n            \n            jsPsych.data.get().addToLast({participant: sbj_id});\n            save_data_json(jsPsych.data.get(),\"full\");\n\n            var relevant_data = jsPsych.data.get().filter([{task: 'dots'},{task: 'choice'},{task: 'confid'}]);\n            relevant_data.addToLast({participant: sbj_id});\n            save_data_json(relevant_data,\"short\");\n\n            \/\/ clear the stage\n            jQuery('display_stage').remove();\n            jQuery('display_stage_background').remove();\n            \/\/ simulate click on Qualtrics \"next\" button, making use of the Qualtrics JS API\n            qthis.clickNextButton();\n        },\n    });\n    }\n});\n\n\nQualtrics.SurveyEngine.addOnReady(function()\n{\n    \/*Place your JavaScript here to run when the page is fully displayed*\/\n\n});\n\nQualtrics.SurveyEngine.addOnUnload(function()\n{\n    \/*Place your JavaScript here to run when the page is unloaded*\/\n\n});","DefaultChoices":false,"QuestionType":"DB","Selector":"TB","DataVisibility":{"Private":false,"Hidden":false},"Configuration":{"QuestionDescriptionOption":"UseText"},"QuestionDescription":"If you are seeing this message for more than 5 minutes, please screen-capture this screen and sen...","ChoiceOrder":[],"Validation":{"Settings":{"Type":"None"}},"GradingData":[],"Language":[],"NextChoiceId":7,"NextAnswerId":1,"QuestionText_Unsafe":"<link href=\"https:\/\/max-lovell.github.io\/jsPsych\/css\/jspsych.css\" rel=\"stylesheet\" type=\"text\/css\"\/>\n<style type=\"text\/css\">#jspsych-canvas-stimulus {\n                position: absolute;\n                top: 0;\n                left: 0;\n                right: 0;\n                margin:  auto;\n            }\n<\/style>\n<div><br\/>\n<br\/>\n<span style=\"font-size: 24px;\">If you are seeing this message for <span style=\"color: rgb(255, 0, 0);\"><b>more than 5 minutes<\/b><\/span>,<br \/>\nplease screen-capture this screen and send the image to us.<br \/>\n<br\/>\n<span style=\"font-size: 28px;\">We are very sorry for the inconvenience.<\/span> <\/span><\/div>\n\n<style type=\"text\/css\">#display_stage_background {\n    width: 100vw;\n    background-color: white;\n    z-index: -1;\n}\n\n#display_stage {\n    position: fixed;\n    left: 1vw;\n    top: 1vh;\n    height: 98vh;\n    width: 98vw;\n    background-color: white;\n    box-shadow: 1px 1px 1px #999;\n    border-radius: 15